  },
  "bugs": {
    "url": "https://github.com/overlookmotel/overlook-framework/issues"
  },
  "dependencies": {
    "express": "^3.21.2",
    "connect-powered-by": "^0.1.0",
    "less-middleware": "^2.2.0",
    "sequelize-extra": "^0.4.13",
    "bluebird-extra": "^2.0.0",
    "continuation-local-storage": "^3.2.0",
    "uuid": "^2.0.2",
    "whoami-exec": "^1.2.2",
    "fs-extra-promise": "^0.4.1",
    "require-folder-tree": "^1.4.5",
    "shimming": "^0.0.8",
    "bcrypt": "^1.0.0",
    "request": "^2.74.0",
    "overlook-utils": "^0.1.0",
    "ejs-extra": "^0.0.1",
    "mandrill-api": "^1.0.45",
    "promisify-any": "^2.0.1",
    "bunyanesque": "^0.2.1",
    "pmx": "^0.6.4"
  },
  "devDependencies": {
    "mocha": "^3.0.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "jshint": "^2.9.3",
    "istanbul": "^0.4.5",
    "coveralls": "^2.11.12",
    "bootstrap": "3.3.7",
    "jquery": "3.1.0",
    "html5shiv": "3.7.3",
    "respond.js": "1.4.2",
    "eonasdan-bootstrap-datetimepicker": "4.17.42",
    "moment": "2.15.0",
    "typeahead.js": "0.11.1",
    "typeahead.js-bootstrap3.less": "git://github.com/hyspace/typeahead.js-bootstrap3.less.git#6b03952",
    "trumbowyg": "^2.3.0"
  },
  "keywords": [
  ],
  "scripts": {
    "test": "npm run jshint && npm run test-main",
    "jshint": "jshint lib test",
    "test-main": "mocha --check-leaks --colors -t 10000 --reporter spec 'test/**/*.test.js'",
    "cover": "npm run cover-main && rm -rf coverage",
    "coveralls": "npm run cover-main && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "cover-main": "COVERAGE=true istanbul cover _mocha --report lcovonly -- -R spec 'test/**/*.test.js'",
    "travis": "if [ $COVERAGE ]; then npm run coveralls; else npm run test; fi"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "readmeFilename": "README.md",
  "license": "MIT"
}
